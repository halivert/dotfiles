[init]
  templatedir = ~/.git_template
  defaultBranch = main
[core]
  autocrlf = input
[pull]
  ff = only
  rebase = false
[merge]
  ff = no
[credential]
  helper = /usr/lib/git-core/git-credential-libsecret
	helper = 
	helper = /usr/local/share/gcm-core/git-credential-manager
[commit]
  gpgSign = true
[push]
  autoSetupRemote = yes
[advice]
  addIgnoredFile = false
